<html>
    <head>
        <title>{{title|e}}</title>
    </head>
    <body>
        <p>{{text|e}}</p>
        {{if(options.projects)}}
        {{each(options.projects)}}
            <a href="{{@this.url|e}}">{{@this.name|e}}</a>
            <p>{{@this.description|e}}</p>
        {{/each}}
        {{#else}}
            No projects
        {{/if}}
    </body>
</html>